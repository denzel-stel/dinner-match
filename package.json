{
  "name": "dinner-match",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
      "./app",
      "./database"
  ],
  "scripts": {
    "dev-api": "tsx ./api/index.ts",
    "start-app": "npm run start --workspace=app",
    "android-app": "npm run android --workspace=app",
    "ios-app": "npm run ios --workspace=app"
  },
  "imports": {
    "#database/*": "./database/*",
    "#services/*": "./services/*",
    "#api/*": "./api/*",
    "#app/*": "./app/*"
  }
}
